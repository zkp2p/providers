        "amount": "$.amount",
        "date": "$.date",
        "paymentId": "$.paymentId",
        "currency": "$.currency"
      }
    },
    "proofMetadataSelectors": [
      {
        "type": "jsonPath",
        "value": "$.stories[{{INDEX}}].amount"
      },
      {
        "type": "jsonPath",
        "value": "$.stories[{{INDEX}}].paymentId"
      },
      {
        "type": "jsonPath",
        "value": "$.stories[{{INDEX}}].title.receiver.username"
      }
    ]
  },
