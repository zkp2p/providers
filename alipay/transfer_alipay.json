    },
    {
      "type": "regex",
      "value": "<td\\s+class=\"amount\">\\s*=?(?<totalAmount>[^<]+)<\\/td>"
    },
    {
      "type": "regex",
      "value": "<td\\s+class=\"time\"[^>]*>\\s*(?<dateCompleted>[^\\r\\n]+)"
    }
  ],
