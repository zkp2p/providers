{
  "actionType": "transfer_revolut",
  "authLink": "https://app.revolut.com/home",
  "url": "https://app.revolut.com/api/retail/user/current/transactions/last?count=20",
  "method": "GET",
  "body": "",
  "metadata": {
    "platform": "revolut",
    "urlRegex": "https://app.revolut.com/api/retail/user/current/transactions/last\\?count=20$",
    "method": "GET",
    "fallbackUrlRegex": "https://app.revolut.com/api/retail/user/current/transactions/last\\?count=20&\\S+",
    "fallbackMethod": "GET",
    "preprocessRegex": "",
    "transactionsExtraction": {
      "transactionJsonPathListSelector": "$",
      "transactionJsonPathSelectors": {
        "recipient": "$.recipient.username",
        "amount": "$.amount",
        "date": "$.completedDate",
        "paymentId": "$.id",
        "currency": "$.currency"
      }
    },
    "proofMetadataSelectors": [
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].amount"
      },
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].currency"
      },
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].recipient.username"
      },
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].completedDate"
      },
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].state"
      },
      {
        "type": "jsonPath",
        "value": "$.[{{INDEX}}].id"
      }
    ]
  },
  "paramNames": [],
  "paramSelectors": [],
  "skipRequestHeaders": [
    "x-device-id",
    "Cookie"
  ],
  "secretHeaders": [
    "x-device-id",
    "Cookie"
  ],
  "responseMatches": [
    {
      "type": "regex",
      "value": "\"amount\":(?<amount>[0-9\\-]+)"
    },
    {
      "type": "regex",
      "value": "\"completedDate\":(?<completedDate>[0-9]+)"
    },
    {
      "type": "regex",
      "value": "\"currency\":\"(?<currency>[^\"]+)\""
    },
    {
      "type": "regex",
      "value": "\"id\":\"(?<id>[^\"]+)\""
    },
    {
      "type": "regex",
      "value": "\"state\":\"(?<state>[^\"]+)\""
    },
    {
      "type": "regex",
      "value": "\"username\":\"(?<username>[^\"]+)\""
    }
  ],
  "responseRedactions": [
    {
      "jsonPath": "$.[{{INDEX}}].amount",
      "xPath": ""
    },
    {
      "jsonPath": "$.[{{INDEX}}].completedDate",
      "xPath": ""
    },
    {
      "jsonPath": "$.[{{INDEX}}].currency",
      "xPath": ""
    },
    {
      "jsonPath": "$.[{{INDEX}}].id",
      "xPath": ""
    },
    {
      "jsonPath": "$.[{{INDEX}}].recipient.username",
      "xPath": ""
    },
    {
      "jsonPath": "$.[{{INDEX}}].state",
      "xPath": ""
    }
  ],
  "processor": {
    "extract": {
      "intentHash": "$.context.contextMessage",
      "amount": "$.context.extractedParameters.amount",
      "date": "$.context.extractedParameters.completedDate",
      "paymentId": "$.context.extractedParameters.id",
      "state": "$.context.extractedParameters.state",
      "receiverId": "$.context.extractedParameters.username",
      "currency": "$.context.extractedParameters.currency"
    },
    "transform": {
      "amountInCents": {
        "input": "amount",
        "ops": [
          { "type": "substring", "start": 1 },
          {
            "type": "conditionalOn",
            "checkField": "currency",
            "if": { "eq": "JPY" },
            "then": [{ "type": "math", "expression": "* 100" }],
            "else": [{ "type": "math", "expression": "* 1" }]
          }
        ]
      },
      "timestamp": {
        "input": "date",
        "ops": [
          { "type": "parseTimestamp" }
        ]
      },
      "receiverIdHashed": {
        "input": "receiverId",
        "ops": ["keccak256"]
      },
      "state": {
        "input": "state",
        "ops": [
          { "type": "assertEquals", "expected": "COMPLETED" }
        ]
      }
    },
    "outputs": [
      { "name": "intentHash", "type": "uint256" },
      { "name": "receiverIdHashed", "type": "bytes32" },
      { "name": "amountInCents", "type": "uint256" },
      { "name": "timestamp", "type": "uint256" },
      { "name": "paymentId", "type": "string" },
      { "name": "currency", "type": "string" }
    ]
  },
  "mobile": {
    "includeAdditionalCookieDomains": [],
    "actionLink": "https://revolut.me/{{RECEIVER_ID}}",
    "isExternalLink": true,
    "appStoreLink": "https://apps.apple.com/us/app/revolut/id370619147",
    "playStoreLink": "https://play.google.com/store/apps/details?id=com.revolut.revolut"
  }
}
